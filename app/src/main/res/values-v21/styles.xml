<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TabTheme" parent="AppTheme">
        <item name="android:colorPrimaryDark">#0D47A1</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:windowAllowEnterTransitionOverlap">true</item>
        <item name="android:windowAllowReturnTransitionOverlap">true</item>
        <item name="android:windowSharedElementEnterTransition">@android:transition/move</item>
        <item name="android:windowSharedElementExitTransition">@android:transition/move</item>

    </style>

    <style name="SignUpTheme" parent="AppTheme">
        <item name="android:colorAccent">#9C27B0</item>
        <item name="android:colorPrimaryDark">#7B1FA2</item>
        <item name="android:colorPrimary">#9C27B0</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:windowAllowEnterTransitionOverlap">true</item>
        <item name="android:windowAllowReturnTransitionOverlap">true</item>
        <item name="android:windowSharedElementEnterTransition">@android:transition/move</item>
        <item name="android:windowSharedElementExitTransition">@android:transition/move</item>

    </style>

    <style name="floatingbtn" parent="Widget.Design.FloatingActionButton">
        <item name="android:colorAccent">#00E676</item>
    </style>

</resources>